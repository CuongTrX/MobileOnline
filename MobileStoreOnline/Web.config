<configuration>
  <system.web>
    <pages validateRequest="false" />
    <compilation debug="true" targetFramework="4.6"/>
    <httpRuntime targetFramework="4.6" requestValidationMode="2.0" />
  </system.web>
  <connectionStrings>
    <add name="MSOConnect" connectionString="Data Source=(LocalDB)\MSSQLLocalDB;AttachDbFilename=|DataDirectory|MobileSO.mdf;Integrated Security=True"/>
  </connectionStrings>
  <appSettings>
    <add key="ValidationSettings:UnobtrusiveValidationMode" value="None"/>
  </appSettings>
